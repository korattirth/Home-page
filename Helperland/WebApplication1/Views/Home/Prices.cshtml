@{
    ViewData["Title"] = "Prices";
}
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<body>
    <link href="~/css/Prices.css" rel="stylesheet" />
    <section>
        <!-- navbar -->
        
        <!-- banner img -->
        <img src="~/img/group-18.png" class="responsive_img" alt="" style="width:100%;margin-top: 70px;">
    </section>

    <!-- prise -->
    <section>
        <div class="container-fluid" style="margin-bottom: 56px;">
            <div class="prise_text">
                <p class="prise">Prices</p>
                <div class="design_star" style="display:flex;justify-content:center">
                    <hr class="hori_line_1">
                    <img src="~/img/separator.png" style="width:24px;height:25px;" alt="">
                    <hr class="hori_line_2">
                </div>
            </div>

            <!-- prise box -->
            <div class="prise_perent_box">
                <div class="prise_box">
                    <div class="prise_box_header">
                        <p class="prise_box_text">One Time</p>
                    </div>
                    <div class="prise_box_body">
                        <div class="body_header_box">
                            <p class="body_header_text"><i class="fa fa-eur doller_sign"></i><span style="font-size: 48px;font-weight: bold;">20</span>/hr</p>
                        </div>
                        <div class="prise_body_text">
                            <div>
                                <p class="body_text"><img src="~/img/forma-10.png" alt="" style="margin-right: 9px;">Lower prices</p>
                                <p class="body_text"><img src="~/img/forma-10.png" alt="" style="margin-right: 9px;">Easy Online & Secure payment</p>
                                <p class="body_text"><img src="~/img/forma-10.png" alt="" style="margin-right: 9px;">Easy amedment</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- hr line -->
            <hr class="hr1">

            <!-- Extra services -->
            <div class="container">
                <div class="row">
                    <div class="col-12" style="margin-bottom: 39px;">
                        <h1 class="extra_ser">Extra Services</h1>
                        <div class="design_star" style="display:flex;justify-content:center">
                            <hr class="hori_line_1"><img src="~/img/separator.png" alt="">
                            <hr class="hori_line_2">
                        </div>
                    </div>

                    <div class="col extra_ser_img_main_div">
                        <div class="first_extra_ser_img">
                            <img src="~/img/forma-13.png" alt="">
                        </div>
                        <p class="extra_ser_text">Inside cabinets</p>
                        <p class="extra_ser_text_1">30 minutes</p>
                    </div>
                    <div class="col extra_ser_img_main_div">
                        <div class="first_extra_ser_img">
                            <img src="~/img/shape-1.png" alt="">
                        </div>
                        <p class="extra_ser_text">Inside fridge</p>
                        <p class="extra_ser_text_1">30 minutes</p>
                    </div>
                    <div class="col extra_ser_img_main_div">
                        <div class="first_extra_ser_img">
                            <img src="~/img/forma-14.png" alt="">
                        </div>
                        <p class="extra_ser_text">Inside oven</p>
                        <p class="extra_ser_text_1">30 minutes</p>
                    </div>
                    <div class="col extra_ser_img_main_div">
                        <div class="first_extra_ser_img">
                            <img src="~/img/forma-12.png" alt="">
                        </div>
                        <p class="extra_ser_text">Laundry wash & dry</p>
                        <p class="extra_ser_text_1">30 minutes</p>
                    </div>
                    <div class="col extra_ser_img_main_div">
                        <div class="first_extra_ser_img">
                            <img src="~/img/forma-12.png" alt="">
                        </div>
                        <p class="extra_ser_text">Insid cadinets</p>
                        <p class="extra_ser_text_1">30 minutes</p>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section>
        <!-- include in cleaning -->
        <div class="container-fluid" style="background-color: #f6f6f6">
            <div class="row cleaning_header" id="blog">
                <div class="col-12">
                    <h1 class="cleaning_header_text">What we include in cleaning</h1>
                    <div class="design_star" style="display:flex;justify-content:center">
                        <hr class="hori_line_1"><img src="~/img/separator.png" alt="">
                        <hr class="hori_line_2">
                    </div>
                </div>
                <div class="col-md-3 cleaning_img_div">
                    <img src="./img/group-19.png" style="width: 100%;" alt="">
                    <p class="inc_cle_text">Bedroom and Living Room</p>
                    <p class="inc_cle_text1"><img src="~/img/vector-smart-object-copy.png" alt="" style="margin-right: 9px;">Dust all accessible surfaces</p>
                    <p class="inc_cle_text1"><img src="~/img/vector-smart-object-copy.png" alt="" style="margin-right: 9px;">down all mirrors and glass fixtures</p>
                    <p class="inc_cle_text1"><img src="~/img/vector-smart-object-copy.png" alt="" style="margin-right: 9px;">Clean all floor surfaces</p>
                    <p class="inc_cle_text1"><img src="~/img/vector-smart-object-copy.png" alt="" style="margin-right: 9px;">out garbage and recycling</p>
                </div>
                <div class="col-md-3 cleaning_img_div">
                    <img src="./img//group-20.png" style="width: 100%;" alt="">
                    <p class="inc_cle_text">Bathrooms</p>
                    <p class="inc_cle_text1"><img src="~/img/vector-smart-object-copy.png" alt="" style="margin-right: 9px;">Wash and sanitize the toilet, shower, tub.</p>
                    <p class="inc_cle_text1"><img src="~/img/vector-smart-object-copy.png" alt="" style="margin-right: 9px;">Dust all accessible surfaces</p>
                    <p class="inc_cle_text1"><img src="~/img/vector-smart-object-copy.png" alt="" style="margin-right: 9px;">Wipe down all mirrors and glass fixtures</p>
                    <p class="inc_cle_text1"><img src="~/img/vector-smart-object-copy.png" alt="" style="margin-right: 9px;">Clean all floor surfaces</p>
                    <p class="inc_cle_text1"><img src="~/img/vector-smart-object-copy.png" alt="" style="margin-right: 9px;">Take out garbage and recycling</p>
                </div>
                <div class="col-md-3 cleaning_img_div">
                    <img src="./img//group-25.png" style="width: 100%;" alt="">
                    <p class="inc_cle_text">Kitchen</p>
                    <p class="inc_cle_text1"><img src="~/img/vector-smart-object-copy.png" alt="" style="margin-right: 9px;">Dust all accessible surfaces</p>
                    <p class="inc_cle_text1"><img src="~/img/vector-smart-object-copy.png" alt="" style="margin-right: 9px;">Empty sink and load up dishwasher</p>
                    <p class="inc_cle_text1"><img src="~/img/vector-smart-object-copy.png" alt="" style="margin-right: 9px;">Wipe down exterior of stove, oven and fridge</p>
                    <p class="inc_cle_text1"><img src="~/img/vector-smart-object-copy.png" alt="" style="margin-right: 9px;">Clean all floor surfaces</p>
                    <p class="inc_cle_text1"><img src="~/img/vector-smart-object-copy.png" alt="" style="margin-right: 9px;">Take out garbage and recycling</p>
                </div>
            </div>
        </div>
    </section>


    <!-- why helperland -->
    <section>
        <div>
            <div class="helpland">
                <div class="healerland_header_div">
                    <h1 class="cleaning_header_text">Why Helperland</h1>
                    <div class="design_star">
                        <hr class="hori_line_1">
                        <img src="./img/separator.png" alt="">
                        <hr class="hori_line_2">
                    </div>
                </div>
                <div class="helperland_perent_div">
                    <div class="helpland_child1">
                        <div>
                            <p class="help_text">Experienced and vetted professionals</p>
                            <p class="help_text_1">dominate the industry in scale and scope with an adaptable, extensive network that consistently delivers exceptional results.</p>
                        </div>
                        <div>
                            <p class="help_text">Dedicated customer service</p>
                            <p class="help_text_1">to our customers and are guided in all we do by their needs. The team is always happy to support you and offer all the information. you need.</p>
                        </div>
                    </div>
                    <div class="helpland_child2">
                        <img src="~/img/the-best-img-1.png" alt="" class="helpland_img_main">
                    </div>
                    <div class="helpland_child3">
                        <div>
                            <p class="help_text">Experienced and vetted professionals</p>
                            <p class="help_text_1">dominate the industry in scale and scope with an adaptable, extensive network that consistently delivers exceptional results.</p>
                        </div>
                        <div>
                            <p class="help_text">Dedicated customer service</p>
                            <p class="help_text_1">to our customers and are guided in all we do by their needs. The team is always happy to support you and offer all the information. you need.</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- get our newsletter -->
        <div class="container-fluid">
            <div class="get_new_perent">
                <div class="get_new_child1">
                    <i class="fa fa-chevron-circle-up"></i>
                </div>
                <div class="get_new_child2">
                    <div class="sellr_main_div">
                        <div style="text-align: center;">
                            <p class="letter_text">GET OUR NEWSLETTER</p>
                            <input type="email" placeholder="Your Email" class="for_email">
                            <input type="submit" class="for_submit">
                        </div>
                    </div>
                </div>
                <div class="get_new_child3">
                    <img src="~/img/layer-598.png" alt="" class="time_img">
                </div>
            </div>
        </div>
    </section>

</body>